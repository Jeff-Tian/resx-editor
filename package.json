{
  "name": "resx-editor",
  "displayName": "Resx Editor",
  "description": "Visual editor for .NET resx files with multi-language support",
  "version": "1.0.0",
  "publisher": "jeff-tian",
  "packageManager": "pnpm@10.26.1+sha512.664074abc367d2c9324fdc18037097ce0a8f126034160f709928e9e9f95d98714347044e5c3164d65bd5da6c59c6be362b107546292a8eecb7999196e5ce58fa",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Jeff-Tian/resx-editor.git"
  },
  "homepage": "https://github.com/Jeff-Tian/resx-editor#readme",
  "bugs": {
    "url": "https://github.com/Jeff-Tian/resx-editor/issues"
  },
  "contributes": {
    "customEditors": [
      {
        "viewType": "resxEditor.editor",
        "displayName": "Resx Editor",
        "selector": [
          {
            "filenamePattern": "*.resx"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "resxEditor.openEditor",
        "title": "Open Resx Editor"
      }
    ],
    "configuration": {
      "title": "Resx Editor",
      "properties": {
        "resxEditor.uiLanguage": {
          "type": "string",
          "default": "auto",
          "enum": [
            "auto",
            "en",
            "zh-cn"
          ],
          "enumDescriptions": [
            "Follow VS Code display language",
            "English",
            "简体中文"
          ],
          "description": "Resx Editor UI language. Default is to follow VS Code display language."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "vsce package"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.107.0",
    "@types/xml2js": "^0.4.14",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "xml2js": "^0.6.2"
  }
}
